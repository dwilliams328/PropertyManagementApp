<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.propertymanagementmobileapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PropertyManagementMobileApp"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.propertymanagementmobileapp.ui.activities.HomeActivity"></activity>
        <activity android:name="com.example.propertymanagementmobileapp.ui.activities.Home" />
        <activity android:name="com.example.propertymanagementmobileapp.ui.auth.LoginActivity" />
        <activity android:name="com.example.propertymanagementmobileapp.ui.auth.RegisterActivity" />
        <activity android:name="com.example.propertymanagementmobileapp.ui.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>